{
  "name": "eslint-plugin-svgo",
  "type": "module",
  "version": "0.8.0",
  "packageManager": "pnpm@10.11.1",
  "description": "Optimize SVG files with SVGO using ESLint.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "svgo"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/eslint-plugin-svgo#readme",
  "repository": "ntnyq/eslint-plugin-svgo",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-plugin-svgo/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./rule-options": {
      "types": "./dts/rule-options.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dts",
    "workers"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run update:rule-options && tsdown",
    "coverage": "vitest --coverage",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "update:rule-options": "tsx scripts/udpateRuleOptions.ts"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "generate-differences": "^0.1.0",
    "svgo": "^4.0.0-rc.4",
    "synckit": "^0.11.8"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.0.0-beta.5",
    "@ntnyq/prettier-config": "^2.2.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.2.1",
    "bumpp": "^10.1.1",
    "eslint": "^9.28.0",
    "eslint-parser-plain": "^0.1.1",
    "eslint-typegen": "^2.2.0",
    "eslint-vitest-rule-tester": "^2.2.0",
    "husky": "^9.1.7",
    "json-schema": "^0.4.0",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "show-invisibles": "^0.0.1",
    "tinyglobby": "^0.2.14",
    "tsdown": "^0.12.6",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.1"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,json}": "eslint --fix"
  }
}
