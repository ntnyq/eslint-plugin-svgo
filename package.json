{
  "name": "eslint-plugin-svgo",
  "type": "module",
  "version": "0.5.0",
  "packageManager": "pnpm@10.2.1",
  "description": "Optimize SVG files with SVGO using ESLint.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "svgo"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/eslint-plugin-svgo#readme",
  "repository": "ntnyq/eslint-plugin-svgo",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-plugin-svgo/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./rule-options": {
      "types": "./dts/rule-options.d.ts"
    },
    "./define-config-support": {
      "types": "./dts/define-config-support.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dts",
    "workers"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run update:rule-options && tsup",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "update:rule-options": "tsx scripts/udpateRuleOptions.ts"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "eslint-formatting-reporter": "^0.0.0",
    "svgo": "^4.0.0-rc.1",
    "synckit": "^0.9.2"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0-beta.3",
    "@ntnyq/prettier-config": "^2.0.0-beta.2",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "bumpp": "^10.0.2",
    "eslint": "9.19.0",
    "eslint-typegen": "^1.0.0",
    "eslint-vitest-rule-tester": "^1.1.0",
    "husky": "^9.1.7",
    "json-schema": "^0.4.0",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.0",
    "tinyglobby": "^0.2.10",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
